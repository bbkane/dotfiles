{
    "[json]": {
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.quickSuggestions": {
            "strings": "on"
        },
        "editor.suggest.insertMode": "replace"
    },
    "[markdown]": {
        "editor._comment": "https://code.visualstudio.com/docs/editor/intellisense , https://stackoverflow.com/a/41324717/2958070",
        "editor.formatOnPaste": false,
        "editor.formatOnSave": false,
        "editor.quickSuggestions": {
            "comments": "on",
            "other": "on",
            "strings": "on"
        },
        "editor.suggest.insertMode": "replace",
        "editor.suggest.localityBonus": true,
        "editor.suggestSelection": "recentlyUsed"
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.formatOnType": true
    },
    "black-formatter.args": [
        "--line-length",
        "100"
    ],
    "dart.previewFlutterUiGuides": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": true,
    "editor.minimap.enabled": false,
    "editor.tokenColorCustomizations": {
        "[Miramare]": {
            "comments": "#c0c0c0"
        }
    },
    "editor.wordBasedSuggestions": true,
    "editor.wordWrap": "on",
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "go.autocompleteUnimportedPackages": true,
    "go.formatTool": "goimports",
    "go.lintOnSave": "workspace",
    "go.lintTool": "golangci-lint",
    "go.toolsManagement.autoUpdate": true,
    "go.useLanguageServer": true,
    "indentRainbow.colors": [
        "#e2987013",
        "#89beba13",
        "#87af8713",
        "#c0c0c013"
    ],
    "indentRainbow.excludedLanguages": [
        "go",
        "json",
        "jsonc",
        "plaintext"
    ],
    "isort.args": [
        "--profile",
        "black"
    ],
    "outline.showVariables": false,
    "problems.defaultViewMode": "table",
    "python._comment": "brew install python3",
    "python.analysis.inlayHints.functionReturnTypes": false,
    "python.analysis.inlayHints.variableTypes": false,
    "python.analysis.typeCheckingMode": "basic",
    "python.defaultInterpreterPath": "/usr/local/bin/python3",
    "python.formatting.provider": "none",
    "python.languageServer": "Pylance",
    "python.linting.enabled": false,
    "redhat.telemetry.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "swaggerViewer.previewInBrowser": true,
    "terminal.integrated.fontSize": 15,
    "window.commandCenter": true,
    "workbench.colorCustomizations": {
        "[Miramare]": {
            "activityBar.border": "#e6dcac3f",
            "activityBar.foreground": "#87af87",
            "activityBar.inactiveForeground": "#e6dcac",
            "editor.findMatchHighlightBackground": "#87af877f",
            "editor.findRangeHighlightBackground._comment": "https://code.visualstudio.com/api/references/theme-color#editor-colors",
            "editorGroup.border": "#e6dcac3f",
            "editorLineNumber.foreground": "#c0c0c09f",
            "panel.border": "#e6dcac3f",
            "panelTitle.activeForeground": "#87af87",
            "panelTitle.inactiveForeground": "#e6dcac",
            "scrollbarSlider.background": "#928374",
            "sideBar.border": "#e6dcac3f",
            "sideBarSectionHeader.border": "#e6dcac3f",
            "tab.activeBorderTop": "#e6dcac3f"
        }
    },
    "workbench.colorTheme": "Default Dark+ Experimental",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.sideBar.location": "right",
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "default"
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.renderWhitespace": "none"
}