{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "lua/?.lua",
      "lua/?/init.lua"
    ]
  },
  "diagnostics": {
    "globals": [
      "vim"
    ],
    "disable": []
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "${3rd}/luv/library"
    ],
    "checkThirdParty": false
  }
}
